CREATE TABLE DadosPessoais (
id  INT NOT NULL    AUTO_INCREMENT,
nome    VARCHAR(32) NOT NULL,
sobrenome    VARCHAR(32) NOT NULL,
sexo  INT NOT NULL,
nascimento  DATE,
email    VARCHAR(32) NOT NULL,
senha    VARCHAR(32) NOT NULL,
corDosOlhos    VARCHAR(32) NOT NULL,
numeroMatricula    VARCHAR(32) NOT NULL,
corCabelo    VARCHAR(32) NOT NULL,
profissao    VARCHAR(32) NOT NULL,
nivelDeEntrada    VARCHAR(32) NOT NULL,
rg    VARCHAR(32) NOT NULL,
cpf    VARCHAR(32) NOT NULL,
PRIMARY KEY (id)
);

CREATE TABLE Acessos (
id  INT NOT NULL    AUTO_INCREMENT,
dadosPessoal_id INT NOT NULL    REFERENCES DadosPessoais(id),
data    DATE,
PRIMARY KEY (id)
);

CREATE TABLE FOTOS(
id INT NOT NULL AUTO_INCREMENT,
dadosPessoal_id INT NOT NULL,
foto1 MEDIUMBLOB,
foto2 MEDIUMBLOB,
foto3 MEDIUMBLOB,
foto4 MEDIUMBLOB,
foto5 MEDIUMBLOB,
foto6 MEDIUMBLOB,
foto7 MEDIUMBLOB,
foto8 MEDIUMBLOB,
foto9 MEDIUMBLOB,
foto10 MEDIUMBLOB,
foto11 MEDIUMBLOB,
foto12 MEDIUMBLOB,
foto13 MEDIUMBLOB,
foto14 MEDIUMBLOB,
foto15 MEDIUMBLOB,
foto16 MEDIUMBLOB,
foto17 MEDIUMBLOB,
foto18 MEDIUMBLOB,
foto19 MEDIUMBLOB,
foto20 MEDIUMBLOB,
foto21 MEDIUMBLOB,
foto22 MEDIUMBLOB,
foto23 MEDIUMBLOB,
foto24 MEDIUMBLOB,
foto25 MEDIUMBLOB,
foto26 MEDIUMBLOB,
foto27 MEDIUMBLOB,
foto28 MEDIUMBLOB,
foto29 MEDIUMBLOB,
foto30 MEDIUMBLOB,
PRIMARY KEY (id)
);
